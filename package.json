{
  "name": "bm-ui",
  "description": "Documentation for BanManager WebUI",
  "version": "1.0.0",
  "author": "James Mortemore (jamesmortemore@gmail.com)",
  "bin": {
    "bm-cli": "./bin/run --writeFile .env"
  },
  "browser": {
    "mobile-detect": false
  },
  "dependencies": {
    "@koa/router": "8.0.8",
    "@nateradebaugh/react-datetime": "4.4.1",
    "@next/bundle-analyzer": "9.3.2",
    "@oclif/command": "1.6.1",
    "@oclif/config": "1.15.1",
    "@oclif/plugin-help": "2.2.3",
    "apollo-server-koa": "2.15.0",
    "apollo-server-plugin-response-cache": "0.5.2",
    "argon2-ffi": "1.2.0",
    "babel-plugin-git-version": "0.2.0",
    "babel-runtime": "6.26.0",
    "compare-versions": "3.6.0",
    "cross-fetch": "3.0.5",
    "dataloader": "2.0.0",
    "date-fns": "2.14.0",
    "db-migrate": "0.11.11",
    "db-migrate-mysql": "2.1.1",
    "dotenv": "8.2.0",
    "edit-dotenv": "1.0.4",
    "file-loader": "6.0.0",
    "graphql": "14.6.0",
    "graphql-constraint-directive": "1.4.1",
    "graphql-depth-limit": "1.1.0",
    "graphql-react": "10.0.0",
    "graphql-tools": "4.0.8",
    "graphql-type-json": "0.3.2",
    "hibp": "9.0.0",
    "inquirer": "7.2.0",
    "js-yaml": "3.14.0",
    "koa": "2.13.0",
    "koa-bodyparser": "4.3.0",
    "koa-pino-logger": "2.1.3",
    "koa-session": "5.13.1",
    "lodash-es": "4.17.15",
    "memoizee": "0.4.14",
    "mobile-detect": "1.4.4",
    "mysql2": "2.1.0",
    "next": "9.3.2",
    "next-absolute-url": "1.2.2",
    "next-compose-plugins": "2.2.0",
    "next-graphql-react": "6.0.1",
    "next-seo": "4.6.0",
    "next-transpile-modules": "3.3.0",
    "pino": "6.3.2",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-grid-layout": "0.18.3",
    "require-environment-variables": "1.2.0",
    "sass": "1.26.9",
    "semantic-ui-css": "2.4.1",
    "semantic-ui-react": "0.88.2",
    "semver": "7.3.2",
    "url-loader": "4.1.0",
    "uuid-parse": "1.1.0",
    "validator": "13.1.1",
    "web-push": "3.4.4"
  },
  "devDependencies": {
    "@oclif/test": "1.2.6",
    "babel-eslint": "10.1.0",
    "coveralls": "3.1.0",
    "eslint-plugin-react": "7.20.0",
    "faker": "4.1.0",
    "jest": "25.5.4",
    "json-to-graphql-query": "2.0.0",
    "mockdate": "2.0.5",
    "nixt": "0.5.1",
    "nock": "12.0.3",
    "nyc": "15.1.0",
    "standard": "14.3.4",
    "supertest": "4.0.2"
  },
  "engines": {
    "node": ">=10.13.0 <13"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "next build",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "dev": "node server.js",
    "heroku-postbuild": "npm run build && node bin/run update",
    "lint": "standard",
    "start": "node server.js",
    "test": "npm run lint && jest --coverage",
    "setup": "node bin/run setup"
  },
  "oclif": {
    "commands": "./cli/commands",
    "bin": "bm-cli",
    "hooks": {
      "init": "./cli/hooks/init"
    },
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "files": [],
  "standard": {
    "parser": "babel-eslint",
    "env": {
      "browser": true
    },
    "globals": [
      "GIT_COMMIT",
      "GIT_TAG",
      "describe",
      "test",
      "beforeAll",
      "afterAll"
    ]
  }
}
