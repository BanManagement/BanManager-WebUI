    "@babel/eslint-parser": "7.17.0",
    "@next/eslint-plugin-next": "12.1.0",
    "@oclif/test": "2.1.0",
    "coveralls-next": "4.1.1",
    "cypress": "9.5.3",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-react": "7.29.4",
    "faker": "5.5.3",
    "jest": "27.5.1",
    "json-to-graphql-query": "2.2.3",
    "mockdate": "3.0.5",
    "nixt": "0.5.1",
    "nock": "13.2.4",
    "standard": "16.0.4",
    "standardx": "7.0.0",
    "supertest": "6.2.2",
    "uuid": "8.3.2"
  },
  "engines": {
    "node": "14 || 16 || 17"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "next build",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "dev": "npx nodemon server.js | npx pino-pretty",
    "heroku-postbuild": "npm run build && node bin/run update",
    "lint": "standardx",
    "start": "node server.js",
    "test": "npm run lint && jest --coverage",
    "setup": "node bin/run setup --writeFile .env",
    "cypress": "cypress open",
    "e2e:build": "node cypress/setup.js && npm run build"
  },
  "oclif": {
    "commands": "./cli/commands",
    "bin": "bm-cli",
    "hooks": {
      "init": "./cli/hooks/init"
    },
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "files": [],
  "standardx": {
    "env": {
      "browser": true
    }
  }
}
