{
  "name": "bm-ui",
  "description": "Documentation for BanManager WebUI",
  "version": "1.0.0",
  "author": "James Mortemore (jamesmortemore@gmail.com)",
  "bin": {
    "bm-cli": "./bin/run --writeFile .env"
  },
  "browser": {
    "mobile-detect": false
  },
  "dependencies": {
    "@artsy/fresnel": "^1.9.0",
    "@koa/router": "8.0.8",
    "@nateradebaugh/react-datetime": "4.4.6",
    "@next/bundle-analyzer": "9.5.5",
    "@oclif/command": "1.8.0",
    "@oclif/config": "1.17.0",
    "@oclif/plugin-help": "3.2.2",
    "apollo-server-koa": "2.24.1",
    "apollo-server-plugin-response-cache": "0.8.0",
    "argon2-ffi": "2.0.0",
    "babel-plugin-git-version": "0.2.0",
    "babel-runtime": "6.26.0",
    "compare-versions": "3.6.0",
    "cross-fetch": "3.1.4",
    "dataloader": "2.0.0",
    "date-fns": "2.21.3",
    "db-migrate": "0.11.12",
    "db-migrate-mysql": "2.1.2",
    "dotenv": "8.6.0",
    "edit-dotenv": "1.0.4",
    "es6-dynamic-template": "2.0.0",
    "file-loader": "6.2.0",
    "graphql": "14.7.0",
    "graphql-constraint-directive": "1.4.1",
    "graphql-depth-limit": "1.1.0",
    "graphql-parse-resolve-info": "4.11.0",
    "graphql-react": "11.2.0",
    "graphql-tools": "4.0.8",
    "graphql-type-json": "0.3.2",
    "hibp": "9.0.3",
    "inquirer": "7.3.3",
    "js-yaml": "3.14.1",
    "knex": "0.95.6",
    "knex-on-duplicate-update": "1.1.3",
    "koa": "2.13.1",
    "koa-bodyparser": "4.3.0",
    "koa-pino-logger": "3.0.0",
    "koa-session": "6.2.0",
    "lodash-es": "4.17.21",
    "memoizee": "0.4.15",
    "mysql2": "github:confuser/node-mysql2#fix/nested-cache-key",
    "next": "10.2.2",
    "next-absolute-url": "1.2.2",
    "next-compose-plugins": "2.2.1",
    "next-graphql-react": "8.0.4",
    "next-seo": "4.24.0",
    "next-transpile-modules": "4.1.0",
    "pino": "6.11.3",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-grid-layout": "0.18.3",
    "require-environment-variables": "1.2.0",
    "sass": "1.34.1",
    "semantic-ui-css": "2.4.1",
    "semantic-ui-react": "0.88.2",
    "semver": "7.3.5",
    "url-loader": "4.1.1",
    "uuid-parse": "1.1.0",
    "validator": "13.6.0",
    "web-push": "3.4.4"
  },
  "devDependencies": {
    "@oclif/test": "1.2.8",
    "babel-eslint": "10.1.0",
    "coveralls": "3.1.0",
    "eslint-plugin-react": "7.23.2",
    "faker": "5.5.3",
    "jest": "26.6.3",
    "json-to-graphql-query": "2.0.0",
    "mockdate": "3.0.5",
    "nixt": "0.5.1",
    "nock": "13.0.11",
    "nyc": "15.1.0",
    "standard": "14.3.4",
    "supertest": "5.0.0"
  },
  "engines": {
    "node": "12"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "next build",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "dev": "node server.js",
    "heroku-postbuild": "npm run build && node bin/run update",
    "lint": "standard",
    "start": "node server.js",
    "test": "npm run lint && jest --coverage",
    "setup": "node bin/run setup --writeFile .env"
  },
  "oclif": {
    "commands": "./cli/commands",
    "bin": "bm-cli",
    "hooks": {
      "init": "./cli/hooks/init"
    },
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "files": [],
  "standard": {
    "parser": "babel-eslint",
    "env": {
      "browser": true
    },
    "globals": [
      "GIT_COMMIT",
      "GIT_TAG",
      "describe",
      "test",
      "beforeAll",
      "afterAll"
    ]
  }
}
