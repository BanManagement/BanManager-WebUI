  "engines": {
    "node": "14 || 16 || 17"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "next build",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "dev": "npx nodemon server.js | npx pino-pretty",
    "heroku-postbuild": "npm run build && node bin/run update",
    "lint": "standardx",
    "start": "node server.js",
    "test": "npm run lint && jest --coverage",
    "setup": "node bin/run setup --writeFile .env",
    "cypress": "cypress open",
    "e2e:build": "node cypress/setup.js && npm run build"
  },
  "oclif": {
    "commands": "./cli/commands",
    "bin": "bm-cli",
    "hooks": {
      "init": "./cli/hooks/init"
    },
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "files": [],
  "standardx": {
    "env": {
      "browser": true
    }
  }
